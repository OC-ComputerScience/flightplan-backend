{
  "name": "esports-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend using Node.js with Rest API",
  "main": "server.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start-alter": "node server.js alter",
    "bundle": "mkdir deploy && cp -r app server.js package.json package-lock.json flight-plan-t1-backend.service .env deploy",
    "winbundle": "md deploy && xcopy app deploy\\app /s /h /i /y /e && copy server.js deploy && copy *.json deploy && copy flightplan-backend.service deploy && copy .env deploy",
    "lint": "eslint --ext .js app",
    "format": "prettier . --write"
  },
  "keywords": [
    "node.js",
    "express",
    "sequelize"
  ],
  "author": "cs",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.17.3",
    "firebase-admin": "^13.2.0",
    "googleapis": "^100.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment-timezone": "^0.5.45",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^7.0.3",
    "sequelize": "^6.19.0",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/eslint-parser": "^7.22.11",
    "@babel/preset-env": "^7.22.10",
    "babel-jest": "^29.7.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.0.3",
    "sequelize-mock": "^0.10.2"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "rules": {},
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false
    },
    "env": {
      "node": true
    }
  },
  "prettier": {}
}
